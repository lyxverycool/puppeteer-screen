{
  "name": "server",
  "version": "0.0.1",
  "description": "puppeteer获取表单截图",
  "scripts": {
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "babel src -d build --copy-files --ignore *.html",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "start:dev": "cross-env NODE_ENV=development node build/server.js",
    "start:prod": "cross-env NODE_ENV=production node build/server.js",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon src/server.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "lyxcool",
  "dependencies": {
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.26.0",
    "node-schedule": "^1.3.2",
    "chalk": "2.3.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.1.3",
    "debug": "^2.6.3",
    "dotenv": "4.0.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-static-router": "^1.5.0",
    "koa-views": "^5.2.1",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "log4js": "^4.1.0",
    "mime-types": "^2.1.33",
    "pug": "^2.0.0-rc.1",
    "puppeteer": "7.1.0",
    "rimraf": "^2.6.3",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "nodemon": "^1.8.1"
  }
}
